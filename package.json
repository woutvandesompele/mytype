{
  "name": "webpack-php-template",
  "version": "1.0.0",
  "description": "template for a webpack project with php",
  "main": "index.js",
  "devDependencies": {
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-devine": "^3.0.1",
    "html-srcsets-loader": "^0.5.1",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^4.0.0",
    "style-loader": "^3.3.0",
    "stylelint-config-devine": "^2.0.3",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "prod": "npm run clean && npm run copydist && webpack --mode production",
